<% if @review.errors.any? %>
  alert("review not blank!, please see again.");
<% else %>
  $("#review_product_<%= @review.product_id %>").html("<%= j render 'products/product_review', product: @review.product %>");
  $(".type_review_<%= @review.product_id %>").val("");
  $(".type_review_<%= @review.product_id %>").attr("placeholder", "Viết bình luận....");
<% end %>
