<%= paginator.render do %>
  <nav aria-label="Page navigation example">
    <ul class="pagination justify-content-center custom-pagination-v1 mb-0">
      <%= prev_page_tag %>
      <% each_page do |page| %>
        <% if page.left_outer? || page.right_outer? || page.inside_window? %>
          <%= page_tag page %>
        <% elsif !page.was_truncated? %>
          <%= gap_tag %>
        <% end %>
      <% end %>
      <%= next_page_tag %>
    </ul>
  </nav>
<% end %>